{
  "name": "videojs-related",
  "version": "0.0.0",
  "description": "End screen for the Brightove Player for related videos",
  "main": "dist/videojs-related.cjs.js",
  "module": "dist/videojs-related.es.js",
  "generator-videojs-plugin": {
    "version": "7.7.3"
  },
  "browserslist": [
    "defaults",
    "ie 11"
  ],
  "scripts": {
    "build": "npm-run-all -s clean -p build:*",
    "build-prod": "cross-env-shell NO_TEST_BUNDLE=1 'npm run build'",
    "build-test": "cross-env-shell TEST_BUNDLE_ONLY=1 'npm run build'",
    "build:css": "postcss -o dist/videojs-related.css --config scripts/postcss.config.js src/plugin.css",
    "build:js": "rollup -c scripts/rollup.config.js",
    "build:lang": "vjslang --dir dist/lang",
    "clean": "shx rm -rf ./dist ./test/dist && shx mkdir -p ./dist ./test/dist",
    "docs": "npm-run-all docs:*",
    "docs:api": "jsdoc src -g plugins/markdown -r -d docs/api",
    "docs:toc": "doctoc --notitle README.md",
    "lint": "vjsstandard",
    "server": "karma start scripts/karma.conf.js --singleRun=false --auto-watch",
    "start": "npm-run-all -p server watch",
    "test": "npm-run-all lint build-test && karma start scripts/karma.conf.js",
    "posttest": "shx cat test/dist/coverage/text.txt",
    "update-changelog": "conventional-changelog -p videojs -i CHANGELOG.md -s",
    "preversion": "npm test",
    "version": "is-prerelease || npm run update-changelog && git add CHANGELOG.md",
    "watch": "npm-run-all -p watch:*",
    "watch:css": "npm run build:css -- -w",
    "watch:js": "npm run build:js -- -w",
    "prepublishOnly": "npm-run-all build-prod && vjsverify --verbose"
  },
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "keywords": [
    "videojs",
    "videojs-plugin"
  ],
  "author": "mister-ben <git@misterben.me>",
  "license": "Apache-2.0",
  "vjsstandard": {
    "ignore": [
      "dist",
      "docs",
      "test/dist"
    ]
  },
  "files": [
    "CONTRIBUTING.md",
    "dist/",
    "docs/",
    "index.html",
    "scripts/",
    "src/",
    "test/"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test"
    }
  },
  "lint-staged": {
    "*.js": [
      "vjsstandard --fix",
      "git add"
    ],
    "README.md": [
      "doctoc --notitle",
      "git add"
    ]
  },
  "dependencies": {
    "global": "^4.3.2",
    "video.js": "^6 || ^7"
  },
  "devDependencies": {
    "@babel/runtime": "^7.5.5",
    "@videojs/generator-helpers": "~1.2.0",
    "jsdoc": "^3.6.10",
    "karma": "^4.0.0",
    "postcss-cli": "^6.0.0",
    "rollup": "^1.12.0",
    "sinon": "^7.2.2",
    "videojs-generate-karma-config": "~5.3.1",
    "videojs-generate-postcss-config": "~2.1.0",
    "videojs-generate-rollup-config": "~5.0.1",
    "videojs-generator-verify": "~2.0.0",
    "videojs-languages": "^2.0.0",
    "videojs-standard": "^8.0.3"
  }
}
